<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.robot.servoscontroller">
    <signal name="error">
      <arg name="bError" type="b" direction="out"/>
    </signal>
    <method name="getName">
      <arg type="s" direction="out"/>
    </method>
    <method name="getAddress">
      <arg type="s" direction="out"/>
    </method>
    <method name="getPluginType">
      <arg type="s" direction="out"/>
    </method>
    <method name="getDescription">
      <arg type="s" direction="out"/>
    </method>
    <method name="isEnabled">
      <arg type="b" direction="out"/>
    </method>
    <method name="setEnabled">
      <arg name="bEnabled" type="b" direction="in"/>
    </method>
    <method name="setServoPulse">
      <arg name="n" type="y" direction="in"/>
      <arg name="pulse" type="d" direction="in"/>
    </method>
    <method name="setAngle">
      <arg name="n" type="y" direction="in"/>
      <arg name="angle" type="q" direction="in"/>
    </method>
  </interface>
</node>
